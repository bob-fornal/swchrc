<html>
  <head>
    <title>SWHRC</title>
    <link rel="stylesheet" type="text/css" href="../index.css?v=" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>
    <div class="wrapper" id="wrapper">
      {% include "header.html" %}

      <div class="content-wrapper">
        <div class="two-column-wrapper">
          <div>
            <div class="header">ARRL Field Day</div>
            {% for site in fieldday.arrlFieldDay %}
              <a class="btn small-text {{ site.classes }}" href="{{ site.link }}" target="_blank" data-filter="{{ site.title }}">{{ site.title }}</a>
              {% if site.gap == true %}
              <div class="gap"></div>
              {% endif %}
            {% endfor %}
          </div>
          <div>
            <div class="header">Winter Field Day</div>
            {% for site in fieldday.winterFieldDay %}
              <a class="btn small-text {{ site.classes }}" href="{{ site.link }}" target="_blank" data-filter="{{ site.title }}">{{ site.title }}</a>
              {% if site.gap == true %}
              <div class="gap"></div>
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>

      {% include 'footer.html' %}
    </div>

  </body>
</html>