<html>
  <head>
    {% include 'head.html' %}
  </head>
  <body>
    <div class="wrapper" id="wrapper">
      {% include "banner.html" %}

      <div class="content-wrapper">
        <div class="header gap">Club Activities</div>
        <div class="two-column-wrapper">
          <div>
            {% for item in activities.left %}
              {% if item.type === 'link' %}
                <a class="btn small-text {{ item.classes }}" href="{{ item.link }}" target="_blank" data-filter="{{ item.title }}">{{ item.title }}</a>
              {% else %}
                <img src="{{ item.src }}" alt="{{ item.alt }}" />
              {% endif %}
            {% endfor %}
          </div>
          <div>
            {% for item in activities.right %}
              {% if item.type === 'link' %}
                <a class="btn small-text {{ item.classes }}" href="{{ item.link }}" target="_blank" data-filter="{{ item.title }}">{{ item.title }}</a>
              {% else %}
                <img src="{{ item.src }}" alt="{{ item.alt }}" />
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>

      {% include 'footer.html' %}
    </div>

  </body>
</html>